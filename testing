while True:
    start_time = time.time()
    today = datetime.date.today()
    today_date = today.strftime("%B %d, %Y")
    job = jobsdb_data('Business analyst', page = 1)
    exist_df = pd.read_notion(test_notion_database_url, api_key=api_key)
    job_list_df = job.information()
    
    applied_lst = []
    
    for index, row in job_list_df.iterrows():
        if row['Website'] not in exist_df['Website']:
            row_dict = row.to_dict()
            result = {'Application_Date':[today_date],'result':['done']}
            row_dict.update(result)
            applied_lst.append(row_dict)
    
    applied_df = pd.DataFrame(data)
    notion_df.upload(applied_df, test_notion_database_url, title="Test", api_key=api_key)
    print(applied_df)
               
    end_time = time.time()
    execution_time = end_time - start_time
    print(execution_time, "sec")
    
    time.sleep(60)
